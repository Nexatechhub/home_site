<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Test_project</servlet-name>
		<servlet-class>com.test.Test_projectServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Test_project</servlet-name>
		<url-pattern>/test_project</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>StockPriceServlet</servlet-name>
		<servlet-class>com.test.StockPriceServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StockPriceServlet</servlet-name>
		<url-pattern>/stock_price</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
